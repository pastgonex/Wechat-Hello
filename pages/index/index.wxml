<!-- latitude 纬度
longitude 经度
scale 规模
enable-overlooking 能够俯视
enable-3D 是否开启3D
markers 标记点 -->
<map name="" class="map" id="map" latitude="{{location.latitude}}" longitude="{{location.longitude}}" scale="{{scale}}" setting="{{setting}}" enable-overlooking="{{isOverLooking}}" enable-3D="{{is3D}}" markers="{{markers}}"></map>